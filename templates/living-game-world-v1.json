{
  "id": "living-game-world-v1",
  "name": "Living Game World Pipeline",
  "description": "Comprehensive 22-agent pipeline for designing complete, interconnected game worlds with emergent systems",
  "version": "1.0.0",
  "category": "game-development",
  "stages": [
    {
      "id": "lore_architect",
      "name": "Lore Architect",
      "type": "designer",
      "agent": "lore_architect",
      "description": "Creates foundational world lore, history, and mythology",
      "phase": "world_building"
    },
    {
      "id": "world_historian",
      "name": "World Historian",
      "type": "reviewer",
      "agent": "world_historian",
      "description": "Validates historical consistency and timeline coherence",
      "phase": "world_building",
      "inputs": ["lore_architect"]
    },
    {
      "id": "geography_designer",
      "name": "Geography Designer",
      "type": "designer",
      "agent": "geography_designer",
      "description": "Designs terrain, biomes, and spatial relationships",
      "phase": "world_building",
      "inputs": ["lore_architect", "world_historian"]
    },
    {
      "id": "ecology_validator",
      "name": "Ecology Validator",
      "type": "reviewer",
      "agent": "ecology_validator",
      "description": "Validates environmental and ecological plausibility",
      "phase": "world_building",
      "inputs": ["geography_designer"]
    },
    {
      "id": "culture_architect",
      "name": "Culture Architect",
      "type": "designer",
      "agent": "culture_architect",
      "description": "Designs societies, cultures, and social systems",
      "phase": "world_building",
      "inputs": ["lore_architect", "world_historian", "geography_designer", "ecology_validator"]
    },
    {
      "id": "sociologist_reviewer",
      "name": "Sociologist Reviewer",
      "type": "reviewer",
      "agent": "sociologist_reviewer",
      "description": "Validates cultural coherence and social plausibility",
      "phase": "world_building",
      "inputs": ["culture_architect"]
    },
    {
      "id": "resource_designer",
      "name": "Resource Designer",
      "type": "designer",
      "agent": "resource_designer",
      "description": "Designs core resources, materials, and currencies",
      "phase": "systems_design",
      "inputs": ["culture_architect", "sociologist_reviewer"]
    },
    {
      "id": "economy_designer",
      "name": "Economy Designer",
      "type": "designer",
      "agent": "economy_designer",
      "description": "Designs trading, markets, and economic systems",
      "phase": "systems_design",
      "inputs": ["resource_designer"]
    },
    {
      "id": "market_simulator",
      "name": "Market Simulator",
      "type": "reviewer",
      "agent": "market_simulator",
      "description": "Simulates and validates economic balance",
      "phase": "systems_design",
      "inputs": ["economy_designer"]
    },
    {
      "id": "combat_designer",
      "name": "Combat Designer",
      "type": "designer",
      "agent": "combat_designer",
      "description": "Designs combat mechanics and encounter systems",
      "phase": "systems_design",
      "inputs": ["resource_designer", "market_simulator"]
    },
    {
      "id": "balance_analyzer",
      "name": "Balance Analyzer",
      "type": "reviewer",
      "agent": "balance_analyzer",
      "description": "Validates combat balance and fairness",
      "phase": "systems_design",
      "inputs": ["combat_designer"]
    },
    {
      "id": "progression_designer",
      "name": "Progression Designer",
      "type": "designer",
      "agent": "progression_designer",
      "description": "Designs leveling, skill trees, and advancement",
      "phase": "systems_design",
      "inputs": ["resource_designer", "balance_analyzer"]
    },
    {
      "id": "engagement_scorer",
      "name": "Engagement Scorer",
      "type": "reviewer",
      "agent": "engagement_scorer",
      "description": "Validates progression feels fun and rewarding",
      "phase": "systems_design",
      "inputs": ["progression_designer"]
    },
    {
      "id": "systems_integrator",
      "name": "Systems Integrator",
      "type": "integrator",
      "agent": "systems_integrator",
      "description": "Combines all systems into cohesive whole",
      "phase": "integration",
      "inputs": ["engagement_scorer"]
    },
    {
      "id": "emergence_detector",
      "name": "Emergence Detector",
      "type": "reviewer",
      "agent": "emergence_detector",
      "description": "Identifies unintended interactions and emergent behaviors",
      "phase": "integration",
      "inputs": ["systems_integrator"]
    },
    {
      "id": "balance_auditor",
      "name": "Balance Auditor",
      "type": "reviewer",
      "agent": "balance_auditor",
      "description": "Final holistic balance check across all systems",
      "phase": "integration",
      "inputs": ["systems_integrator", "emergence_detector"]
    },
    {
      "id": "player_experience_simulator",
      "name": "Player Experience Simulator",
      "type": "reviewer",
      "agent": "player_experience_simulator",
      "description": "Simulates actual player journeys and experiences",
      "phase": "integration",
      "inputs": ["systems_integrator", "balance_auditor"]
    },
    {
      "id": "data_modeler",
      "name": "Data Modeler",
      "type": "executor",
      "agent": "data_modeler",
      "description": "Creates data schemas and database designs",
      "phase": "implementation",
      "inputs": ["player_experience_simulator"]
    },
    {
      "id": "api_designer",
      "name": "API Designer",
      "type": "executor",
      "agent": "api_designer",
      "description": "Designs system APIs and interfaces",
      "phase": "implementation",
      "inputs": ["data_modeler"]
    },
    {
      "id": "code_generator",
      "name": "Code Generator",
      "type": "executor",
      "agent": "code_generator",
      "description": "Generates implementation code from specifications",
      "phase": "implementation",
      "inputs": ["api_designer"]
    },
    {
      "id": "technical_validator",
      "name": "Technical Validator",
      "type": "validator",
      "agent": "technical_validator",
      "description": "Validates technical implementation quality",
      "phase": "validation",
      "inputs": ["code_generator"]
    },
    {
      "id": "gameplay_validator",
      "name": "Gameplay Validator",
      "type": "validator",
      "agent": "gameplay_validator",
      "description": "Validates gameplay implementation matches design",
      "phase": "validation",
      "inputs": ["code_generator"]
    },
    {
      "id": "narrative_validator",
      "name": "Narrative Validator",
      "type": "validator",
      "agent": "narrative_validator",
      "description": "Validates world and narrative coherence",
      "phase": "validation",
      "inputs": ["code_generator"]
    },
    {
      "id": "final_integrator",
      "name": "Final Integrator",
      "type": "validator",
      "agent": "final_integrator",
      "description": "Final holistic validation and launch readiness",
      "phase": "validation",
      "inputs": ["technical_validator", "gameplay_validator", "narrative_validator"]
    }
  ],
  "flow": {
    "type": "conditional",
    "description": "Complex multi-phase pipeline with extensive feedback loops and cross-dependencies",
    "connections": [
      {
        "from": "lore_architect",
        "to": "world_historian",
        "condition": "lore_complete",
        "description": "Submit lore for historical validation"
      },
      {
        "from": "world_historian",
        "to": "geography_designer",
        "condition": "approved",
        "description": "Lore approved, proceed to geography"
      },
      {
        "from": "world_historian",
        "to": "lore_architect",
        "condition": "needs_revision",
        "description": "Historical inconsistencies found, revise lore"
      },
      {
        "from": "geography_designer",
        "to": "ecology_validator",
        "condition": "geography_complete",
        "description": "Submit geography for ecological validation"
      },
      {
        "from": "ecology_validator",
        "to": "culture_architect",
        "condition": "approved",
        "description": "Geography validated, design cultures"
      },
      {
        "from": "ecology_validator",
        "to": "geography_designer",
        "condition": "needs_revision",
        "description": "Ecological issues found, revise geography"
      },
      {
        "from": "ecology_validator",
        "to": "lore_architect",
        "condition": "lore_conflict",
        "description": "Geography conflicts with lore, need lore revision"
      },
      {
        "from": "culture_architect",
        "to": "sociologist_reviewer",
        "condition": "culture_complete",
        "description": "Submit cultures for sociological review"
      },
      {
        "from": "sociologist_reviewer",
        "to": "resource_designer",
        "condition": "approved",
        "description": "Cultures approved, begin systems design"
      },
      {
        "from": "sociologist_reviewer",
        "to": "culture_architect",
        "condition": "needs_revision",
        "description": "Cultural issues found, revise"
      },
      {
        "from": "sociologist_reviewer",
        "to": "geography_designer",
        "condition": "geography_conflict",
        "description": "Culture-geography mismatch, revise geography"
      },
      {
        "from": "sociologist_reviewer",
        "to": "lore_architect",
        "condition": "lore_conflict",
        "description": "Culture-lore mismatch, revise lore"
      },
      {
        "from": "resource_designer",
        "to": "economy_designer",
        "condition": "resources_complete",
        "description": "Resources defined, design economy"
      },
      {
        "from": "resource_designer",
        "to": "combat_designer",
        "condition": "resources_complete",
        "description": "Resources defined, design combat (parallel)"
      },
      {
        "from": "resource_designer",
        "to": "progression_designer",
        "condition": "resources_complete",
        "description": "Resources defined, design progression (parallel)"
      },
      {
        "from": "economy_designer",
        "to": "market_simulator",
        "condition": "economy_complete",
        "description": "Submit economy for simulation"
      },
      {
        "from": "market_simulator",
        "to": "combat_designer",
        "condition": "approved",
        "description": "Economy validated, inform combat design"
      },
      {
        "from": "market_simulator",
        "to": "economy_designer",
        "condition": "needs_tuning",
        "description": "Economic imbalance found, retune"
      },
      {
        "from": "market_simulator",
        "to": "resource_designer",
        "condition": "resource_issue",
        "description": "Resource design causes economic problems"
      },
      {
        "from": "combat_designer",
        "to": "balance_analyzer",
        "condition": "combat_complete",
        "description": "Submit combat for balance analysis"
      },
      {
        "from": "balance_analyzer",
        "to": "progression_designer",
        "condition": "approved",
        "description": "Combat balanced, inform progression"
      },
      {
        "from": "balance_analyzer",
        "to": "combat_designer",
        "condition": "needs_tuning",
        "description": "Combat imbalance found, retune"
      },
      {
        "from": "balance_analyzer",
        "to": "economy_designer",
        "condition": "economy_conflict",
        "description": "Combat rewards conflict with economy"
      },
      {
        "from": "balance_analyzer",
        "to": "resource_designer",
        "condition": "resource_issue",
        "description": "Combat resource costs are problematic"
      },
      {
        "from": "progression_designer",
        "to": "engagement_scorer",
        "condition": "progression_complete",
        "description": "Submit progression for engagement analysis"
      },
      {
        "from": "engagement_scorer",
        "to": "systems_integrator",
        "condition": "approved",
        "description": "Progression engaging, ready for integration"
      },
      {
        "from": "engagement_scorer",
        "to": "progression_designer",
        "condition": "needs_improvement",
        "description": "Progression not engaging enough, revise"
      },
      {
        "from": "engagement_scorer",
        "to": "combat_designer",
        "condition": "combat_conflict",
        "description": "Progression conflicts with combat pacing"
      },
      {
        "from": "engagement_scorer",
        "to": "economy_designer",
        "condition": "economy_conflict",
        "description": "Progression costs conflict with economy"
      },
      {
        "from": "systems_integrator",
        "to": "emergence_detector",
        "condition": "integration_complete",
        "description": "Systems integrated, check for emergence"
      },
      {
        "from": "systems_integrator",
        "to": "balance_auditor",
        "condition": "integration_complete",
        "description": "Systems integrated, audit overall balance"
      },
      {
        "from": "systems_integrator",
        "to": "player_experience_simulator",
        "condition": "integration_complete",
        "description": "Systems integrated, simulate player experience"
      },
      {
        "from": "emergence_detector",
        "to": "balance_auditor",
        "condition": "clean",
        "description": "No major emergence issues, proceed to audit"
      },
      {
        "from": "emergence_detector",
        "to": "systems_integrator",
        "condition": "major_exploits",
        "description": "Major exploits found, re-integrate"
      },
      {
        "from": "emergence_detector",
        "to": "combat_designer",
        "condition": "combat_exploit",
        "description": "Combat system exploit found"
      },
      {
        "from": "emergence_detector",
        "to": "economy_designer",
        "condition": "economy_exploit",
        "description": "Economy exploit found"
      },
      {
        "from": "emergence_detector",
        "to": "progression_designer",
        "condition": "progression_exploit",
        "description": "Progression exploit found"
      },
      {
        "from": "balance_auditor",
        "to": "player_experience_simulator",
        "condition": "balanced",
        "description": "Balance approved, verify player experience"
      },
      {
        "from": "balance_auditor",
        "to": "systems_integrator",
        "condition": "unbalanced",
        "description": "Balance issues across systems, re-integrate"
      },
      {
        "from": "balance_auditor",
        "to": "combat_designer",
        "condition": "combat_issue",
        "description": "Combat balance issue"
      },
      {
        "from": "balance_auditor",
        "to": "economy_designer",
        "condition": "economy_issue",
        "description": "Economy balance issue"
      },
      {
        "from": "balance_auditor",
        "to": "progression_designer",
        "condition": "progression_issue",
        "description": "Progression balance issue"
      },
      {
        "from": "player_experience_simulator",
        "to": "data_modeler",
        "condition": "smooth",
        "description": "Player experience validated, begin implementation"
      },
      {
        "from": "player_experience_simulator",
        "to": "systems_integrator",
        "condition": "problematic",
        "description": "UX issues found, re-integrate"
      },
      {
        "from": "player_experience_simulator",
        "to": "progression_designer",
        "condition": "onboarding_issue",
        "description": "Onboarding/tutorial problems"
      },
      {
        "from": "data_modeler",
        "to": "api_designer",
        "condition": "schemas_complete",
        "description": "Data models ready, design APIs"
      },
      {
        "from": "api_designer",
        "to": "code_generator",
        "condition": "apis_complete",
        "description": "APIs designed, generate code"
      },
      {
        "from": "code_generator",
        "to": "technical_validator",
        "condition": "code_complete",
        "description": "Code generated, validate technically"
      },
      {
        "from": "code_generator",
        "to": "gameplay_validator",
        "condition": "code_complete",
        "description": "Code generated, validate gameplay"
      },
      {
        "from": "code_generator",
        "to": "narrative_validator",
        "condition": "code_complete",
        "description": "Code generated, validate narrative"
      },
      {
        "from": "technical_validator",
        "to": "final_integrator",
        "condition": "approved",
        "description": "Technical validation passed"
      },
      {
        "from": "technical_validator",
        "to": "code_generator",
        "condition": "needs_revision",
        "description": "Technical issues found, regenerate code"
      },
      {
        "from": "technical_validator",
        "to": "api_designer",
        "condition": "api_issue",
        "description": "API design issue found"
      },
      {
        "from": "technical_validator",
        "to": "data_modeler",
        "condition": "schema_issue",
        "description": "Data model issue found"
      },
      {
        "from": "gameplay_validator",
        "to": "final_integrator",
        "condition": "approved",
        "description": "Gameplay validation passed"
      },
      {
        "from": "gameplay_validator",
        "to": "code_generator",
        "condition": "needs_work",
        "description": "Gameplay implementation issues"
      },
      {
        "from": "gameplay_validator",
        "to": "systems_integrator",
        "condition": "design_issue",
        "description": "Implementation reveals design problem"
      },
      {
        "from": "narrative_validator",
        "to": "final_integrator",
        "condition": "approved",
        "description": "Narrative validation passed"
      },
      {
        "from": "narrative_validator",
        "to": "code_generator",
        "condition": "needs_revision",
        "description": "Narrative inconsistencies in implementation"
      },
      {
        "from": "narrative_validator",
        "to": "culture_architect",
        "condition": "culture_issue",
        "description": "Cultural implementation doesn't match design"
      },
      {
        "from": "narrative_validator",
        "to": "lore_architect",
        "condition": "lore_issue",
        "description": "Lore inconsistencies found in implementation"
      },
      {
        "from": "final_integrator",
        "to": "code_generator",
        "condition": "needs_more_work",
        "description": "Polish needed, iterate on implementation"
      },
      {
        "from": "final_integrator",
        "to": "systems_integrator",
        "condition": "systemic_issue",
        "description": "Fundamental design issue, re-integrate systems"
      }
    ]
  },
  "success_criteria": {
    "final_verdict": "APPROVED_FOR_LAUNCH",
    "all_validations": "passed",
    "no_critical_issues": true,
    "implementation_complete": true
  },
  "use_cases": [
    "Design complete MMO game worlds with interconnected systems",
    "Create comprehensive single-player RPG designs",
    "Develop complex strategy game economies and progression",
    "Build living, breathing game worlds with emergent gameplay",
    "Design and validate entire game features from concept to code"
  ],
  "example_prompts": [
    "Design a complete space exploration MMO with mining, trading, combat, and faction systems",
    "Create a medieval fantasy RPG world with economy, crafting, questing, and political intrigue",
    "Design a post-apocalyptic survival game with resource scarcity, base building, and PvP",
    "Build a cyberpunk city with gangs, corporations, hacking, and street-level combat"
  ],
  "metadata": {
    "total_agents": 24,
    "feedback_loops": 37,
    "max_depth": 8,
    "estimated_runtime": "8-20 hours depending on complexity",
    "recommended_for": "Major game features, complete world designs, full game loops"
  }
}
